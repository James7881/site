    <!-- build:css /css/style.min.css -->
    {{#is 'atomicguide' this.basename}}
    <link rel="stylesheet" href="/css/style.css">
    {{/is}}
    {{#any 'landing, casestudy' this.basename}}
    <link rel="stylesheet" href="/css/site.css">
    {{/any}}
    <!-- endbuild -->
  </head>
  <body
  {{#is 'atomicguide' this.basename}}
  class="atomic"
  {{/is}}>

  <header role="banner">
    {{#any 'landing, atomicguide' this.basename}}
    {{> logo}}
    {{/any}}
    {{> nav}}
  </header>

  <main role="main">
    {% body %}
  </main>

  {{#any 'landing, atomicguide' this.basename}}
  {{> footer}}
  {{/any}}

  {{> jquery}}

  <!-- build:js js/src/site.min.js -->
  <script src="js/lib/ffobserver.js"></script>
  <script src="js/src/ffobserver.js"></script>
  <!-- endbuild -->

  <script class="atomic-js">var circle = document.querySelectorAll('div.socials__btn');

for(var i = 0, l = circle.length; i < l; i++) {
  circle[i].addEventListener('click', function() {
    this.parentNode.classList.toggle('is-bloomed');
  });
}
</script>

  <script src="js/src/testimonial-rotator.js"></script>

  {{#is 'atomicguide' this.basename}}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.16.1/TweenMax.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.1/ScrollMagic.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.1/plugins/animation.gsap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.16.1/plugins/ScrollToPlugin.min.js"></script>

  <!-- build:js js/src/atomic-guide.min.js -->
  <script src="js/src/scrollmation.js"></script>
  <script src="js/lib/prism.js"></script>
  <script src="js/src/atomic-swatch.js"></script>
  <script src="js/src/atomic-generator.js"></script>
  <!-- endbuild -->
  {{/is}}

